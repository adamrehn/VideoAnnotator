{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"lib": ["es6", "dom"],
		"sourceMap": true,
		"strict": true,
		"strictNullChecks": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"moduleResolution": "Node",
		"rootDir": "./source/ts",
		"outDir": "./source/js"
	}
}
